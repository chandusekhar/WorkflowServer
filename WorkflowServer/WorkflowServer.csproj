<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp2.0</TargetFramework>
    <ApplicationIcon>icon.ico</ApplicationIcon>
    <Win32Resource />
    <SignAssembly>false</SignAssembly>
    <Product>WorkflowServer</Product>
    <PackageLicenseUrl>
    </PackageLicenseUrl>
    <PackageProjectUrl>https://workflowengine.io/server/</PackageProjectUrl>
    <Authors>OptimaJet LLC</Authors>
    <Company>OptimaJet LLC</Company>
    <Version>2.4.3</Version>
    <PublishProtocol>FileSystem</PublishProtocol>
    <PublishDir>bin\Release\PublishOutput</PublishDir>
  </PropertyGroup>
  <ItemGroup>
    <None Update="config.json">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="WorkflowEngine.NET-Server" Version="2.4.5" />
    <PackageReference Include="Microsoft.Extensions.Configuration.Binder" Version="2.1.1" />
    <PackageReference Include="Microsoft.Extensions.Configuration.EnvironmentVariables" Version="2.1.1" />
    <PackageReference Include="Microsoft.Extensions.Configuration.Json" Version="2.1.1" />
  </ItemGroup>
</Project>